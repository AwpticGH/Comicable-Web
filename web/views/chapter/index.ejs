<div class="kontenn">
        <div class="chapter-navigation chapter-navigation-top">
                <a href="<%= api_data.pagination.previous %>"> < </a>
                <h3><%= api_data.title %></h3>
                <a href="<%= api_data.pagination.next %>"> > </a>
        </div>
        <%
        let length = api_data.images.length;
        for (let i = 0; i < length; i++) {
        %>
        <img src="<%= api_data.images[i] %>" alt="<%= `Gambar ${i + 1}` %>">
        <% } %>
        <div class="chapter-navigation chapter-navigation-bottom">
                <a href="<%= api_data.pagination.previous %>"> < </a>
                <h3><%= api_data.title %></h3>
                <a href="<%= api_data.pagination.next %>"> > </a>
        </div>
</div>